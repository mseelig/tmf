<h1>Listing phonemaps</h1>

<table>
  <tr>
    <th>Guest phone</th>
    <th>Restaurant code</th>
  </tr>

<% @phonemaps.each do |phonemap| %>
  <tr>
    <td><%=h phonemap.guest_phone %></td>
    <td><%=h phonemap.restaurant_code %></td>
    <td><%= link_to 'Show', phonemap %></td>
    <td><%= link_to 'Edit', edit_phonemap_path(phonemap) %></td>
    <td><%= link_to 'Destroy', phonemap, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New phonemap', new_phonemap_path %>